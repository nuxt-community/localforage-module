{
  "name": "@nuxtjs/localforage",
  "version": "1.1.0",
  "description": "Localforage module for Nuxt.js",
  "repository": "nuxt-community/localforage-module",
  "license": "MIT",
  "contributors": [
    "Alexandre Chopin <alex@orion.sh>",
    "Ricardo Gobbo de Souza <ricardogobbosouza@yahoo.com.br>"
  ],
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "lint": "eslint --ext .js,.vue,.ts .",
    "release": "yarn lint && standard-version && git push --follow-tags && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.0.0-rc.11",
    "defu": "^6.1.0",
    "localforage": "^1.10.0"
  },
  "devDependencies": {
    "@nuxt/module-builder": "latest",
    "@nuxtjs/eslint-config-typescript": "latest",
    "eslint": "latest",
    "nuxt": "^3.0.0-rc.11",
    "standard-version": "^9.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
